{
    "model": {
        "type": "image_v2",
        "input_channels": 3,
        "input_size": [256, 256],
        "patch_size": 1,
        "mapping_out": 256,
        "depths": [2, 4, 4],
        "channels": [128, 256, 512],
        "self_attn_depths": [false, false, false],
        "has_variance": true,
        "loss_config": "karras",
        "loss_weighting": "sqrt-snr",
        "dropout_rate": 0.05,
        "augment_wrapper": true,
        "augment_prob": 0.12,
        "sigma_data": 0.5,
        "sigma_min": 1e-2,
        "sigma_max": 160,
        "sigma_sample_density": {
            "type": "cosine"
        },
        "ortho_tf_type": "dwt"
    },
    "dataset": {
        "type": "imagefolder",
        "location": "../data/train/ffhq256"
    },
    "optimizer": {
        "type": "adamw",
        "lr": 1e-4,
        "betas": [0.95, 0.999],
        "eps": 1e-6,
        "weight_decay": 0
    },
    "lr_sched": {
        "type": "constant",
        "warmup": 0.0
    },
    "ema_sched": {
        "type": "inverse",
        "power": 0.6667,
        "max_value": 0.9999
    }
}