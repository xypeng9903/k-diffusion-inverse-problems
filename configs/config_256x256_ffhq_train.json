{
    "model": {
        "input_channels": 3,
        "input_size": [256, 256],
        "sigma_min": 1e-2,
        "sigma_max": 80,
        "sigma_sample_density": {
            "type": "lognormal",
            "mean": -1.2,
            "std": 1.2
        },
        "has_variance": true,
        "augment_wrapper": true,
        "augment_prob": 0.12,
        "openai": {
            "num_channels": 128,
            "num_res_blocks": 1,
            "attention_resolutions": "16"
        }
    },
    "dataset": {
        "type": "imagefolder",
        "location": "../../dataset/ffhq256"
    },
    "optimizer": {
        "type": "adamw",
        "lr": 1e-4,
        "betas": [0.95, 0.999],
        "eps": 1e-6,
        "weight_decay": 1e-3
    },
    "lr_sched": {
        "type": "constant"
    },
    "ema_sched": {
        "type": "inverse",
        "power": 0.6667,
        "max_value": 0.9999
    }
}
